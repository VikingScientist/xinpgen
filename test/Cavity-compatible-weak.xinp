<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<simulation>

  <geometry>
    <raiseorder patch="1" u="0" v="0"/>
    <refine patch="1" u="3" v="3"/>
    <topologysets>
      <set name="lid" type="edge">
        <item patch="1">4</item>
      </set>
      <set name="bottom" type="edge">
        <item patch="1">3</item>
      </set>
      <set name="vertical" type="edge">
        <item patch="1">1 2</item>
      </set>
      <set name="horizontal" type="edge">
        <item patch="1">3 4</item>
      </set>
    </topologysets>
  </geometry>

  <stokes>
    <boundaryconditions>
      <dirichlet set="vertical" basis="1" comp="1"/>
      <dirichlet set="horizontal" basis="2" comp="1"/>
      <neumann set="lid" direction="-1" type="generic">1|0</neumann>
      <neumann set="bottom" direction="-1" type="generic">0|0</neumann>
      <neumann set="vertical" direction="-1" type="generic">0|0</neumann>
    </boundaryconditions>
    <constrain_integrated_pressure/>
    <fluidproperties mu="1.0" rho="1.0"/>
    <gravity x="0" y="0" z="0"/>
    <formulation>stress</formulation>
  </stokes>

</simulation>
