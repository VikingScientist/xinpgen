<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<--! 2D pressure-driven channel flow. Half model. !-->

<simulation>

  <geometry X0="0.0 0.0 0.0" Lx="2.0" Ly="0.4">
    <refine patch="1" u="79" v="79"/>
    <topologysets>
      <set name="inflow" type="edge" closure="open">
        <item patch="1">1</item>
      </set>
      <set name="lower wall" type="edge">
        <item patch="1">3</item>
      </set>
      <set name="upper wall" type="edge">
        <item patch="1">4</item>
      </set>
    </topologysets>
  </geometry>

  <boundaryconditions>
    <neumann set="inflow" direction="1">50000.0</neumann>
    <dirichlet set="inflow" comp="2"/>
    <dirichlet set="lower wall" comp="1" axes="local"/>
    <dirichlet set="upper wall" comp="12"/>
  </boundaryconditions>

  <stokes>
    <fluidproperties rho="1.0" mu="1.0"/>
  </stokes>

</simulation>
